                        <Insets bottom="4.0"/>
                    </VBox.margin>
                </Button>
                <Button id="button-test" fx:id="exitButton" alignment="CENTER" mnemonicParsing="false"
                        onAction="#closeApplication" prefWidth="230.0" style="-fx-background-radius: 10;"
